{
  "name": "karunais13/laravel-onesignal",
  "description": "OneSignal Push Wrapper Laravel with Proxy support",
  "keywords": ["onesignal", "webpush", "push", "laravel", "Proxy"],
  "type": "library",
  "require": {
    "php": ">=5.4.0",
    "guzzlehttp/guzzle": "^6.2|^7.4.1",
    "illuminate/support": "~5.5|~6.0|~7.0|~8.0|~9.0",
    "symfony/psr-http-message-bridge": "1.*|2.*"
  },
  "require-dev": {
    "vlucas/phpdotenv": "^2.2"
  },
  "autoload": {
    "psr-4": {
      "Karu\\OneSignal\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Karu\\OneSignal\\OneSignalServiceProvider"
      ],
      "aliases": {
        "OneSignal": "Karu\\OneSignal\\OneSignalFacade"
      }
    }
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Karunaiswaran Sivaguru",
      "email": "karunais1392@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true
}
